{
  "name": "simple-es/ramsey-uuid-bridge",
  "description": "A bridge for using Rhumsaa Uuid to generate UUIDs",
  "type": "library",
  "keywords": ["simple", "simplees", "simple-es", "uuid"],
  "homepage": "https://simple-es.github.io",
  "license": "MIT",
  "authors": [
    {
      "name": "Jasper N. Brouwer",
      "email": "jasper@future500.nl"
    }
  ],

  "require": {
    "php": ">=5.4",
    "simple-es/event-sourcing": "~1.0@dev",
    "rhumsaa/uuid": "~2.8"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.3",
    "squizlabs/php_codesniffer": "~2.1"
  },

  "autoload": {
    "psr-4": {"SimpleES\\RamseyUuidBridge\\": "src"}
  },
  "autoload-dev": {
    "psr-4": {"SimpleES\\RamseyUuidBridge\\Test\\": "tests"}
  },

  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
